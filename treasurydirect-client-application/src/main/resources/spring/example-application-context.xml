<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="securitiesExample1Resource" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg name="path" value="/examples/securitiesSingleResultExample.groovy"/>
    </bean>

    <bean id="securitiesExample2Resource" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg name="path" value="/examples/securitiesMultipleResultsExample.groovy"/>
    </bean>

    <bean id="securitiesMultipleResultsWithDataFrameExampleResource" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg name="path" value="/examples/securitiesMultipleResultsWithDataFrameExample.groovy"/>
    </bean>

    <bean id="debtsMultipleResultsWithDataFrameExampleResource" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg name="path" value="/examples/debtsMultipleResultsWithDataFrameExample.groovy"/>
    </bean>
</beans>
